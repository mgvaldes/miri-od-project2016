<?xml version="1.0"?>
<metadata>
    <prefixes>
        <prefix name="rest">
            <namespace>http://restaurants.recommender.es/property/restaurant/</namespace>
            <classes>
                <class name="Restaurant">
                    <properties>
                        <property name="name" />
                        <property name="belongsTo" />
                        <property name="location" />
                        <property name="rate" />
                        <property name="contact" />
                    </properties>
                </class>
            </classes>
        </prefix>
        <prefix name="loc">
            <namespace>http://restaurants.recommender.es/property/location/</namespace>
            <classes>
                <class name="Location">
                    <properties>
                        <property name="address" />
                        <property name="city" />
                        <property name="country" />
                        <property name="latitude" />
                        <property name="longitude" />
                    </properties>
                </class>
            </classes>
        </prefix>
        <prefix name="rate">
            <namespace>http://restaurants.recommender.es/property/rate/</namespace>
            <classes>
                <class name="Rate">
                    <properties>
                        <property name="price" />
                        <property name="score" />
                        <property name="likes" />
                    </properties>
                </class>
            </classes>
        </prefix>
        <prefix name="cont">
            <namespace>http://restaurants.recommender.es/property/contact/</namespace>
            <classes>
                <class name="Contact">
                    <properties>
                        <property name="timetable" />
                        <property name="telephone" />
                        <property name="email" />
                    </properties>
                </class>
            </classes>
        </prefix>
        <prefix name="res">
            <namespace>http://restaurants.recommender.es/resource/</namespace>
        </prefix>
    </prefixes>
